{
    "name": "wordcoolframework/cool-dump",
    "description": "CoolDump is Debug Tool",
    "version": "1.1.0",
    "type": "library",
    "keywords": ["cool dump", "wtf", "cool dumper"],
    "license": "MIT",
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "CoolDump\\": "src/",
            "Configuration\\": "src/Configuration"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\CoolDump\\Tests\\": "src/Test/"
        }
    },
    "authors": [
        {
            "name": "wordcoolframework",
            "email": "wordcool.framework@gmail.com"
        }
    ],

    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    }
}
